{
    "name": "moonscript",
    "displayName": "Moonscript Language",
    "description": "Moonscript Language support",
    "version": "1.1.5",
    "readme": "README.md",
    "publisher": "Lynxish-Developer",
    "repository": {
        "type": "git",
        "url": "https://github.com/owenkimbrell/Visual-Studio-Code-Moonscript-Syntax"
    },
    "engines": {
        "vscode": "^1.1.0"
    },
    "categories": [
        "Languages",
        "Other",
        "Snippets"
    ],
    "icon": "CatWithAblunt.jpg",
    "license": "./LICENSE",
    "keywords": [
        "moonscript",
        "moon",
        ".moon",
        "Syntax",
        "Language"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "moonscript",
                "aliases": [
                    "MoonScript",
                    "moonscript",
                    "moon"
                ],
                "extensions": [
                    ".moon"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "moonscript",
                "scopeName": "source.moonscript",
                "path": "./syntaxes/MoonScriptxx.json",
                "injectTo": [
                    "moon",
                    "moonscript"
                ]
            }
        ],
        "snippets": [
            {
                "language": "moonscript",
                "path": "./snippets/MoonScriptxx-Snippets.json"
            }
        ]
    },
    "dependencies": {
        "@types/node": "^8.0.7",
        "shelljs": "^0.8.5",
        "tslint": "^5.4.3",
        "typescript": "^2.4.1",
        "vscode": "^1.1.1"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "activationEvents": [
        "onLanguage:moonscript"
    ],
    "devDependencies": {
        "util": "^0.10.3"
    }
}
